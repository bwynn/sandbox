doctype html
html(ng-app="myApp")
  head
    title Top Ten Competitors for 2015
    script(src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.0-rc.0/angular.js")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.5.0-rc.0/angular-route.js")
    script(src="/static/js/app.js")
  body
    div(ng-controller="myCtrl")
      h2 {{world}}
      div(ng-repeat="person in competitors")
        h2 {{person.name}}
